{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "/": "-------- DEV --------",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start-reset-cache": "react-native start --reset-cache",
    "//": "-------- SETUP --------",
    "bootstrap": "yarn && yarn init-gem && yarn pod-install",
    "install-dependencies": "yarn && yarn pod-install",
    "init-gem": "bundle install",
    "pod-install": "cd ios && bundle exec pod install && cd ..",
    "///": "-------- LINTING AND CHECKS --------",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint-debug": "eslint ./src --ext .js,.jsx,.ts,.tsx --debug",
    "prettier-check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "ts-compile": "tsc -p tsconfig.json --noEmit",
    "pre-push-lint": "yarn lint --max-warnings 0 && yarn ts-compile",
    "////": "----------- UTILITIES ----------",
    "pre-push": "yarn prettier-check && yarn pre-push-lint",
    "postinstall": "test -f .env || cp -n .env.example .env",
    "prepare": "husky",
    "clean": "bash ./scripts/clean.sh"
  },
  "dependencies": {
    "@react-navigation/native": "6.1.17",
    "@react-navigation/native-stack": "6.9.26",
    "@react-navigation/stack": "6.3.29",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "3.30.1",
    "styled-components": "6.1.8"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@types/react": "18.2.6",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.19.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-ft-flow": "3.0.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-jest": "27.9.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "9.0.11",
    "prettier": "2.8.8",
    "react-native-dotenv": "3.4.11",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
